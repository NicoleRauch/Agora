{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },

  "env": {
    "es6": true,
    "node": false,
    "browser": true,
    "jquery": true
  },
  "rules": {
    "camelcase": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "strict": [
      2,
      "function"
    ]
  },
  "plugins": [
    "import"
  ]
}
